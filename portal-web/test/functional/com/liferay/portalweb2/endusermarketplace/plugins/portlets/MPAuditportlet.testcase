<definition component-name="marketplace-plugins-audit-portlet">
	<set-up>
		<execute macro="User#firstLoginPG" />
	</set-up>

	<command name="AuditPortletSmoke" priority="5">
		<property name="portlet.plugins.includes" value="audit-portlet" />

		<var name="appName" value="Audit" />

		<execute macro="Page#gotoCP">
			<var name="portletName" value="Audit Reports" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#uninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>

		<execute macro="Page#gotoCP">
			<var name="portletName" value="App Manager" />
		</execute>

		<execute macro="AppManager#viewUninstallAppCP">
			<var name="appName" value="${appName}" />
		</execute>
	</command>
</definition>